{
  "name": "ubermanu/kek",
  "description": "Another PHP micro-framework",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Emmanuel Vodor",
      "email": "manu@paca.happy-dev.fr"
    }
  ],
  "autoload": {
    "psr-4": {
      "Kek\\": "src/"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Kek\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.0.0",
    "ext-mbstring": "*",
    "ext-pdo": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
